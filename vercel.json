{
  "version": 2,
  "buildCommand": "node scripts/vercel-build.js",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(admin|subjects|categories|question-interface|analytics|settings|privacy|terms|contact|cookie-settings|our-story)",
      "dest": "/index.html"
    },
    {
      "src": "/categories/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}